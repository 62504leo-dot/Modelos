   <string notr="true">/* === Estilo general === */
QMainWindow {
    background-color: #f6f7fb;
    color: #333333;
    font-family: "Segoe UI Variable", "Roboto", "Open Sans", sans-serif;
    font-size: 16px;
    font-weight: 300;
}

/* === Frames === */
QFrame {
    background-color: #ffffff;
    border-radius: 10px;
    border: 1px solid #dcdcdc;
}

/* Barra superior */
#frame_superior {
    background-color: #e8ecf1;
    border: none;
}

/* Panel lateral */
