    background-color: #f1f3f6;
    border-right: 1px solid #d1d1d1;
}

/* Área principal */
#frame_paginas {
    background-color: #ffffff;
}

/* === Botones === */
QPushButton {
    background-color: #f5f5f5;
    color: #333333;
    border: 1px solid #cccccc;
    border-radius: 6px;
    padding: 8px 14px;
    font-weight: 400;
    font-size: 15px;
}

QPushButton:hover {
    background-color: #0078d7;
    color: #ffffff;
    border: 1px solid #0078d7;
}

QPushButton:pressed {
    background-color: #005a9e;
    color: #ffffff;
    border: 1px solid #005a9e;
}

/* Botón principal (Menú) */
#boton_menu {
    background-color: #0078d7;
    color: white;
    border: none;
    font-weight: 400;
}
#boton_menu:hover {
    background-color: #006cbe;
}
#boton_menu:pressed {
    background-color: #005a9e;
}

/* Botón salir */
#boton_salir {
    background-color: #e74c3c;
    color: white;
    border: none;
}
#boton_salir:hover {
    background-color: #c0392b;
}
#boton_salir:pressed {
    background-color: #a93226;
}

/* === Botones laterales === */
#btn_lc_1, #btn_lc_2, #btn_lc_3, #btn_lc_4, #btn_lc_5, #btn_lc_6, #btn_lc_7, #btn_lc_8 {
