    color: #333333;
    font-weight: 400;
    font-size: 15px;
    text-align: left;
    padding-left: 20px;
}

#btn_lc_1:hover, #btn_lc_2:hover, #btn_lc_3:hover, #btn_lc_4:hover, #btn_lc_5:hover, #btn_lc_6:hover, #btn_lc_7:hover, #btn_lc_8:hover {
    background-color: #e8f0fe;
    border: 1px solid #0078d7;
    color: #0078d7;
}

/* === Campos de texto === */
QLineEdit, QTextEdit {
    background-color: #ffffff;
    color: #333333;
    border: 1px solid #cccccc;
    border-radius: 4px;
    padding: 6px 8px;
    font-size: 15px;
}

QLineEdit:focus, QTextEdit:focus {
    border: 1px solid #0078d7;
    background-color: #f9fcff;
}

/* === Labels === */
QLabel {
    color: #333333;
    font-weight: 300;
    font-size: 15px;
    border: none;
}
